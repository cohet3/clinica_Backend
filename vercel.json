{
    "version": 2,
    "name": "petgram-Danny",
    "builds": [
        { "use": "@vercel/node", "src": "package.json"}
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"            
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"            
        },
        {
            "src": "/.*",
            "dest": "index.html"
        }
    ]
} 