{
    "version": 2,
    "name": "petgram-Danny",
    "builds": [
        { "src": "./index.js",
            "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"            
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"            
        },
        {
            "src": "/.*",
            "dest": "index.html"
        }
    ]
} 